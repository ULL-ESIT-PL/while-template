{
  "name": "scope-intro",
  "version": "1.0.0",
  "description": "A lab for PL. Intro to Scope Analysis",
  "main": "src/transpile.js",
  "bin": {
    "calc2js": "./bin/calc2js.js"
  },
  "scripts": {
    "test": "npm run compile; mocha test/test.mjs; mocha test/test-error.js",
    "only-test": "mocha --inspect-brk only-test/test.mjs",
    "cov": "c8 --reporter=html -o docs -x src/support-lib.js npm run test",
    "test1": "npm run compile; ./bin/calc2js.mjs test/data/test1.calc ",
    "compile": "jison src/grammar.jison src/lexer.l -o src/calc.js",
    "clean": "rm -f src/calc.js; rm -fR coverage",
    "comment": "Use -- for the following scripts, i.e. npm run mocha -- -scope2",
    "mocha": "test/scripts/t.sh mocha",
    "check": "test/scripts/t.sh check",
    "ready": "test/scripts/t.sh ready",
    "calc2js": "bin/calc2js.mjs",
    "while-ast": "compast test/data/expectedjs/while.js | yq '.body[2].expression.expressions[1].right'"
  },
  "keywords": [
    "ull",
    "jison",
    "espree",
    "compiler"
  ],
  "author": "Casiano Rodriguez Leon <crguezl@ull.edu.es> (https://crguezl.github.io/)",
  "license": "ISC",
  "dependencies": {
    "ast-types": "^0.14.2",
    "c8": "^9.1.0",
    "chmod": "^0.2.1",
    "commander": "^10.0.0",
    "complex.js": "^2.1.1",
    "dotenv": "^16.4.5",
    "jison": "^0.4.18",
    "recast": "^0.22.0",
    "set-operations": "^2.0.2",
    "shelljs.exec": "^1.1.8"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "shelljs": "^0.8.5"
  }
}
